<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.product.bean.Employee" table="elk_loglist">

		<id name="empno" type="int" column="empno">
			<generator class="increment" />
			<!-- <generator class="sequence"> <param name="sequence">MySeq</param> 
				</generator> -->
		</id>

		<property name="ename" length="10" />
		<property name="sal" />
		<property name="desig" type="string" column="designation"
			length="10" />

	</class>
</hibernate-mapping>